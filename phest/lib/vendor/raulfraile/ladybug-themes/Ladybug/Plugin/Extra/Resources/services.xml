            <argument type="service" id="type_extended_factory"></argument>
        </service>

        <service id="plugin.extra.inspector.php.resource.mysql_link" class="Ladybug\Plugin\Extra\Inspector\Resource\Php\MysqlLink">
            <tag name="ladybug.inspector" />
